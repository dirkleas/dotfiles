select-pane -t 0 # Select the first (0) pane.
split-window -v -l 35 # Split it vertically. Omit -p 75 or replace with -l and the number of lines if needed.
select-pane -t 0 # Reselect the first (0) pane.
split-window -h # Split it horizontally. Omit -p 50 or replace with -l and the number of columns if needed.
select-pane -t 2 # Select the bottom pane (now it might be pane 1 depending on the layout).